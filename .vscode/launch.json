{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Edge with Emscripten",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:8080/index.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "preLaunchTask": "Clean Configure and Build with emcmake",
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*"
      }
    }
  ]
}